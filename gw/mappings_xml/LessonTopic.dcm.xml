<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LessonTopic" table="lesson_topic">
    <id name="lessonTopicId" type="integer" column="lesson_topic_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lessonId" type="integer" column="lesson_id" nullable="false"/>
    <field name="topicId" type="integer" column="topic_id" nullable="false"/>
    <field name="topicCreatedAt" type="datetime" column="topic_created_at" nullable="false"/>
  </entity>
</doctrine-mapping>
