<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Lesson" table="lesson">
    <id name="lessonId" type="integer" column="lesson_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lessonTypeId" type="integer" column="lesson_type_id" nullable="false"/>
    <field name="lessonPageurl" type="string" column="lesson_pageurl" length="2083" nullable="false"/>
    <field name="lessonDetail" type="text" column="lesson_detail" nullable="false"/>
    <field name="lessonCreatedAt" type="datetime" column="lesson_created_at" nullable="false"/>
  </entity>
</doctrine-mapping>
